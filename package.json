{
  "name": "@levibostian/action-sync-branches",
  "version": "0.1.0",
  "description": "replace me",
  "scripts": {
    "test": "npx jest",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "lint": "npx eslint --fix --ext ts,js . && npx tsc --noEmit",
    "compile": "npx ncc build app/index.ts -o dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levibostian/action-node-blanky.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Levi Bostian <levi@levi.earth>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/levibostian/action-node-blanky/issues"
  },
  "homepage": "https://github.com/levibostian/action-node-blanky#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "npm run format"
    ]
  },
  "prettier": "@levibostian/prettier-config",
  "devDependencies": {
    "@levibostian/eslint-config-node": "^1.3.1",
    "@levibostian/jest-config": "^1.0.1",
    "@levibostian/prettier-config": "^1.0.0",
    "@levibostian/tsconfigs": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.7.0",
    "eslint-plugin-jest": "^25.7.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@actions/core": "^1.6.0"
  }
}
